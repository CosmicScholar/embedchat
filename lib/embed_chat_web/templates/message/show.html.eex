<h2>Show message</h2>

<ul>

  <li>
    <strong>Body:</strong>
    <%= @message.body %>
  </li>

  <li>
    <strong>From:</strong>
    <%= @message.from_id %>
  </li>

  <li>
    <strong>To:</strong>
    <%= @message.to_id %>
  </li>

  <li>
    <strong>Room:</strong>
    <%= @message.room_id %>
  </li>

</ul>

<%= link "Edit", to: message_path(@conn, :edit, @message) %>
<%= link "Back", to: message_path(@conn, :index) %>
